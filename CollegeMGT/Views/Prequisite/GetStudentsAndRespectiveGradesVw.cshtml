@model IEnumerable<CollegeMGT.Core.Views.ThirdPrequisiteVw>

<br />
<div class="row">
    <div class="col-6">
        <h2 class="text-dark">Prequisite </h2>
           <p class="text-dark">- List of students and there respective grades of subject</p>
    </div>
    <div class="col-6 text-right">
        <a class="btn btn-primary" asp-controller="Student" asp-action="Upsert"><i class="fas fa-plus"></i> &nbsp; Add New Student</a>
    </div>
</div>

<br />
<div class="p-4 border rounded">
    <table id="tblData" class="table table-striped table-bordered" style="width:100%">
        <thead class="thead-dark">
            <tr class="table-info">
                <th>Student Name</th>
                 <th>Subject Name</th>
                  <th>Grade Value</th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts{ 
    <script src="~/js/getStudentsAndRespectiveGrades.js"></script>
}